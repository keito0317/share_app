<h2>新規登録</h2>

<%= form_with model: @room, multipart: true do |f| %>
<table>
<tr>
<th><%= f.label :room_name ,"施設名" %></th>
<td><%= f.text_field :room_name %></td>
</tr>
<tr>
<th><%= f.label :details ,"施設詳細" %></th>
<td><%= f.text_area :details %></td>
</tr>
<tr>
<th><%= f.label :price ,"料金" %></th>
<td><%= f.text_field :price %></td>
</tr>
<tr>
<th><%= f.label :address ,"住所" %></th>
<td><%= f.text_field :address %></td>
</tr>
<td><%= f.file_field :image %></td>
<tr>
<td><%= f.submit "保存"%></td>
</tr>
</table>



<div>
     <ul>
       <li><%= link_to "キャンセル", :rooms %></li>
     </ul>
   </div>

<% end %>